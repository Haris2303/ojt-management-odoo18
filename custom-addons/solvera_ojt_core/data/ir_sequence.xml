<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_ojt_batch" model="ir.sequence">
            <field name="name">OJT Batch Sequence</field>
            <field name="code">ojt.batch</field>
            <field name="prefix">OJTBATCH/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_ojt_certificate" model="ir.sequence">
            <field name="name">OJT Certificate Sequence</field>
            <field name="code">ojt.certificate</field>
            <field name="prefix">OJTCERT/%(year)s/</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>