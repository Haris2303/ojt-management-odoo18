<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="survey_survey_view_form_inherit_ojt" model="ir.ui.view">
        <field name="name">survey.survey.view.form.inherit.ojt</field>
        <field name="model">survey.survey</field>
        <field name="inherit_id" ref="survey.survey_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='participants']" position="inside">
                <field name="batch_id"/>
            </xpath>
        </field>
    </record>
</odoo>